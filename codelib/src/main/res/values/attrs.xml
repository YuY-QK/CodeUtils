<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean" />
        <attr name="max_select" format="integer" />
        <attr name="hor_spacing" format="dimension" />
        <attr name="ver_spacing" format="dimension" />
        <attr name="gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FilterCheckBox">
        <attr name="text" format="string" />
        <attr name="textSelectedColor" format="color" />
        <attr name="textUnSelectColor" format="color" />
        <attr name="checked" format="boolean" />
    </declare-styleable>
</resources>